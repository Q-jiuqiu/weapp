.toolsbar {
  position: fixed;
  z-index: 9;
  top: 70%;
  right: 2%;
  font-size: 20rpx
}

.toolsbar .order-box {
  position: relative;
  margin-bottom: 12rpx
}

.toolsbar .order-box .sub-list>view, .toolsbar .order-box .order {
  background-color: #70d0cf;
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  color: #ffffff;
  box-shadow: 0px 0px 5px #888888;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center
}

.toolsbar .order-box .consult-button {
  padding: 0;
  font-size: 20rpx;
  overflow: hidden;
  border-radius: 50%;
  color: white;
  background-color: #1c1f24;
  font-weight: normal
}

.toolsbar .order-box .order-order {
  -webkit-animation: haha1 0.8s linear 1
}

.toolsbar .order-box .order-cancle {
  -webkit-animation: haha1 0.8s linear 1
}

.toolsbar .order-box .sub-list {
  position: absolute;
  bottom: 100%
}

.toolsbar .order-box .sub-list>view {
  background-color: #1c1f24;
  margin-bottom: 12rpx
}

.toolsbar .my {
  color: #c2c2c2;
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  background-color: #ffffff;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}

.toolsbar .my .box {
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.toolsbar .my-button {
  background-color: transparent;
  align-items: center;
  font-weight: normal;
  font-size: 20rpx;
  border-radius: 50%;
  width: 95rpx;
  box-shadow: 0px 0px 5px #888888;
  height: 95rpx;
  padding: 0;
}

.toolsbar .label-icon {
  display: flex;
  justify-content: center
}

@-webkit-keyframes haha1 {
  0% {
    -webkit-transform: rotate(0deg)
  }
  100% {
    -webkit-transform: rotate(180deg)
  }
}