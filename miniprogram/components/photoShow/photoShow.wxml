<!-- components/photoShow.wxml -->
<view class="photoShow">
  <view class="photoShow-box">
    <view class="photo" wx:for="{{photoList}}" wx:key="index" bind:tap="goToDetail" data-index="{{ index }}">
      <image class="img" src="{{item.src}}" mode="widthFix" lazy-load="true" style="width:100%;"></image>
      <view class="title">{{item.title}}</view>
      <view class="time">{{item.time}}</view>
    </view>
  </view>
</view>